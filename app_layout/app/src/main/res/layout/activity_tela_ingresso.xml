<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#2B2E4A"
    tools:context=".TelaIngresso">

    <TextView
        android:id="@+id/textViewNav"
        android:layout_width="match_parent"
        android:layout_height="40dp"
        android:layout_marginBottom="59dp"
        android:background="@drawable/shp_radius"
        android:text=""
        app:circularflow_radiusInDP="10dp"

        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <View
        android:id="@+id/viewDescIngresso"
        android:layout_width="311dp"
        android:layout_height="158dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_marginBottom="112dp"
        android:background="@drawable/descricaoIngresso"
        app:circularflow_radiusInDP="5dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <View
        android:id="@+id/imgIngressoFilme"
        android:layout_width="310dp"
        android:layout_height="319dp"
        android:background="@color/black"
        app:circularflow_radiusInDP="5dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        tools:layout_editor_absoluteY="142dp" />

    <View
        android:id="@+id/viewCircular1"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:layout_margin="16dp"
        android:background="@drawable/circle"
        tools:layout_editor_absoluteX="335dp"
        tools:layout_editor_absoluteY="441dp" />

    <View
        android:id="@+id/viewCircular2"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:layout_margin="16dp"
        android:background="@drawable/circle"
        tools:layout_editor_absoluteX="25dp"
        tools:layout_editor_absoluteY="441dp" />



</androidx.constraintlayout.widget.ConstraintLayout>